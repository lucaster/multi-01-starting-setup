docker network create goals-net
docker stop goals-frontend goals-backend mongodb
docker build -t goals-node  ./backend
docker build -t goals-react ./frontend
docker run --name mongodb        -d  --rm --network goals-net              mongo
docker run --name goals-backend  -d  --rm --network goals-net -p 80:80     goals-node
docker run --name goals-frontend -it --rm --network goals-net -p 3000:3000 goals-react


-it necessario per react npm start